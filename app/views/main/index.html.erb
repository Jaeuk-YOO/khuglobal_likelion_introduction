        <script>
            $( document ).ready(function() {
                $("#lotto_check").click(function(){
                    $("#lotto_result").empty();
                                                   
                    var lotto_array = [$("input[id=num1]").val(),$("input[id=num2]").val(),$("input[id=num3]").val(),$("input[id=num4]").val(),$("input[id=num5]").val(),$("input[id=num6]").val()];
                    var chk_count=0
                    var bns_count=0
                    
                    <%@lotto_number.each do |x|%>
                        if (lotto_array.includes('<%=x%>')==true){
                            chk_count=chk_count+1;
                        }
                        
                    <%end%>
                    
                    if (lotto_array.includes('<%=@bonus_number%>')==true){
                        bns_count=bns_count+1;
                    }
                    
                    <%[6,5,4,3,2,1].each_with_index do |y, index|%>
                    if (chk_count==<%=y%> && bns_count == 0) {
                        $( "#lotto_result" ).append( "<p><%=index+1%>등입니다! <%=y%>개 맞추셨습니다.</p>" );
                    }
                
                    else if (bns_count == 1 && chk_count==<%=y%>) {
                        $( "#lotto_result" ).append( "<p>2등입니다! 5개, 보너스 번호 맞추셨습니다.</p>" );
                    }
                    <%end%>
                });
            });
        </script>

    
